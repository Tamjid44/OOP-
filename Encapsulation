#---Encapsulation---
class vehicle:
  def __init__(self, brand):
    self.brand= brand
    print(f"{self.brand} is a nice car")
class Nissan(vehicle):
  def __init__(self, brand, model):
    self.brand = brand
    self.model = model
    print(f"{self.brand} {self.model} is a cool car")
obj = Nissan("Nissan", "GTR")
obj.brand
obj.model
