#---Abstraction---
from abc import ABC , abstractmethod
class parent:
  def accost(self, name):
    self.name = name
    print(f"{self.name} is my Father")
class child(parent):
  def address(self):
      return "Dad"
      
obj = child()
obj.accost("Jack")
obj.address()
child = child()
print(child.address())
